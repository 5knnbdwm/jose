{
  "name": "ultimate-jose",
  "version": "0.0.1",
  "description": "JSON Web Everything (JWA, JWS, JWE, JWK, JWKS, JWT) for Node.js with minimal dependencies",
  "keywords": [
    "jose",
    "jsonwebtoken",
    "jwa",
    "jwe",
    "jwk",
    "jws",
    "jwt",
    "rfc7515",
    "rfc7516",
    "rfc7517",
    "rfc7518",
    "rfc7519",
    "rfc7797"
  ],
  "homepage": "https://github.com/panva/ultimate-jose",
  "repository": "panva/ultimate-jose",
  "license": "MIT",
  "author": "Filip Skokan <panva.ip@gmail.com>",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "coverage": "nyc ava",
    "lint": "standard",
    "lint-fix": "standard --fix",
    "test": "ava",
    "watch": "ava --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@trust/keyto": "^0.3.4",
    "ms": "^2.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.1",
    "@commitlint/config-conventional": "^7.5.0",
    "ava": "^1.2.1",
    "husky": "^1.3.1",
    "nyc": "^13.2.0",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">=11.8.0"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "test/**/*.test.js"
    ]
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
